    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Deploy staging",
            "program": "${workspaceRoot}/node_modules/ember-cli/bin/ember",
            "args": [
              "electron"
            ]
        },
        { 
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost:4200/",
            "sourceMapPathOverrides":
            { 
              "ember-test/": "${workspaceRoot}/app/"
            }
        }
    ]
}